so ~/.vim/plugins.vim


" Configuration
syntax enable

set tabstop=8 softtabstop=0 expandtab shiftwidth=4 smarttab     "Redefine tabs with 4 spaces
set backspace=indent,eol,start          "Make backlash behave like every other editor


" Visual
set number              " Show the line numbers
colorscheme github
set background=light

" Mapping
:imap jk <Esc> 	        " Type <jk> to exit insert mode
